* {
  font-family: 'Microsoft YaHei';
}

.QFrame
{
    background-color: rgb(255, 255, 255);
    border-radius: 10px;
}

.QWidget#widget {
  background-color: rgb(223, 232, 242);
}

.QWidget#widget_2 {
  background-color: rgb(23, 35, 59);
}

.QLabel#soft_ware_name {
  color: rgb(77, 81, 101);
  font-size: 11pt;
}

.QPushButton#btn_start {
  border-style: none;
  color: rgb(0, 0, 0);
  background-color: rgb(255, 255, 255);
  font-size: 10pt;
  border-radius: 5px;
}

.QPushButton#btn_start:hover
{
  background-color: rgba(223, 232, 242);
}

.QPushButton#btn_start:pressed
{
    background-color: rgb(217, 227, 238);
}

.QPushButton#btn_sec2_inroomrate
{
  color: rgb(0, 0, 0);
  background-color: rgb(225, 242, 253);
  font-size: 9pt;
  border-color: rgba(156, 140, 140, 0.65);
  border-style: outset;
  border-width: 1px;
  border-radius: 5px;
}

.QPushButton#btn_sec2_inroomrate:hover
{
  background-color: rgba(223, 232, 242);
}

.QPushButton#btn_sec2_inroomrate:pressed
{
    background-color: rgb(217, 227, 238);
}
